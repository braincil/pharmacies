<!DOCTYPE html>
<html>
<head>
  <title>Pharm</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% City.all.each do |city| %>
	<%=link_to city.name, pharmacies_path(city: city.name)%>
<%end%>
<%= yield %>

</body>
</html>
